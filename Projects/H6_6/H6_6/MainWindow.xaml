<Window x:Class="H6_6.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:H6_6"
        mc:Ignorable="d"
        Title="MainWindow" Height="350" Width="800">
    <Grid>
        <Rectangle x:Name="minutes" Fill="#FFFF4A0D" HorizontalAlignment="Left" Height="44" Margin="0,44,0,0" Stroke="Black" VerticalAlignment="Top" Width="0" />
        <Rectangle x:Name="seconds" Fill="#FF0909F3" HorizontalAlignment="Left" Height="44" Margin="0,160,0,0" Stroke="Black" VerticalAlignment="Top" Width="0"/>

    </Grid>
</Window>
